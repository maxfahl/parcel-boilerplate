{
	"directories": {
		"lib": "dist"
	},
	"author": "Max Fahl",
	"license": "MIT",
	"private": true,
	"files": [
		"lib/",
		"src/"
	],
	"name": "fileselector",
	"version": "1.0.0",
	"description": "A simple app that let's the user select and play a file in related program.",
	"main": "lib/index.js",
	"scripts": {
		"prestart": "rm -rf ../public/fileselector && mkdir ../public/fileselector && cp -R ./src/static ../public/fileselector/static",
		"start": "parcel serve --out-dir ../public/fileselector --public-url /public/fileselector --no-hmr src/index.html",
		"prebuild": "rm -rf ../public/fileselector && mkdir ../public/fileselector && cp -R ./src/static ../public/fileselector/static",
		"build": "cross-env NODE_ENV=production parcel build --no-source-maps --out-dir ../public/fileselector --public-url . src/index.html",
		"watch": "parcel watch src/index.html",
		"predeploy": "npm run build"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"gh-pages": "^1.2.0",
		"node-sass": "^4.7.2",
		"parcel-bundler": "^1.9.7",
		"typescript": "^2.9.2"
	},
	"dependencies": {
		"normalize-scss": "^7.0.1"
	}
}
